   "metadata": {},
   "outputs": [],
   "source": [
    "def query_big_query(query_string):\n",
    "    client = bigquery.Client()\n",
    "    query_job = client.query(query_string)\n",
    "\n",
    "    results = query_job.result()  # Waits for job to complete.\n",
    "    return results"
   ]
  },
