    "            group by DAY_Datetime\n",
    "            order by DAY_Datetime\n",
    "            )\n",
    "\n",
    "            select *,\n",
    "            total_amount - LAG(total_amount) OVER (ORDER BY DAY_Datetime) AS Difference\n",
    "            FROM daily;\n",
    "            \n",
